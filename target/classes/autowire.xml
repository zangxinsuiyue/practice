<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="jobs" class="com.spring.entity.Job">
        <property name="jobName" value="teacher"/>
    </bean>
    <bean id="job2" class="com.spring.entity.Job">
        <property name="jobName" value="student"/>
    </bean>
    <bean id="user" class="com.spring.entity.User" autowire="byType"/>
</beans>